{
  "name": "devcontainer",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "mounts": [
    {
      "source": "${localWorkspaceFolder}",
      "target": "/workspace/emoji2text",
      "type": "bind"
    }
  ],
  "customizations": {
    "vscode": {
      "extensions": ["ms-python.python", "ms-python.autopep8"]
    }
  },
  "remoteUser": "user"
}
